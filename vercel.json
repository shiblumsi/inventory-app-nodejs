{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "service/swagger.yaml",
            "prisma/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  